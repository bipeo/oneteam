
@font-face {
  font-family: 'HelveticaNeue-Bold';
  src: url("../fonts/HelveticaNeue-Bold.eot");
  src: url("../fonts/HelveticaNeue-Bold.eot?#iefix") format('embedded-opentype'), 
  url("../fonts/HelveticaNeue-Bold.woff") format('woff'), url("../fonts/HelveticaNeue-Bold.ttf") format('truetype'), 
  url("../fonts/HelveticaNeue-Bold.svg#HelveticaNeue-Bold") format('svg');
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: 'HelveticaNeue-Light';
  src: url("../fonts/HelveticaNeue-Light.eot");
  src: url("../fonts/HelveticaNeue-Light.eot?#iefix") format('embedded-opentype'), 
  url("../fonts/HelveticaNeue-Light.woff") format('woff'), url("../fonts/HelveticaNeue-Light.ttf") format('truetype'), 
  url("../fonts/HelveticaNeue-Light.svg#HelveticaNeue-Light") format('svg');
  font-weight: normal;
  font-style: normal;
}

.container
    max-width 1001px;
    width 100%
    margin 0 auto
    &:after
        content ''
        display block
        clear both
.wrapper
    &:after
        content ''
        display block
        clear both
.wrapper-contant
    min-height: 100%;
    position: relative;
.icon
    background url(../img/icons.png) no-repeat
    display inline-block
.logo
        float left
        margin 0 14px 0 6px
        width 184px
        height 59px
        text-indent -9999px
        overflow hidden
        background url("../img/logo.png") 50% 0 no-repeat
        -webkit-background-size 184px 59px
        background-size 184px 59px
        margin 0 64px 0 0
        &:hover
            opacity 0.8
        a
            display block
            height 100%
.phone
    float right
    margin 4px 0 0 0
    font-size 18px
    font-family: 'HelveticaNeue-Bold';
    color #1e2226
    padding 0 0 0 24px
    position relative
    .icon
        position: absolute;
        top: 13px;
        left: -8px;
        width: 23px;
        height: 23px;
        background-position: -17px 0;
        &:after
            content: '';
            width: 90px;
            height: 1px;
            background: #dcb57b;
            display: inherit;
            margin: 11px 0 0 23px;
    p
        margin-bottom 3px
.top-block
    background #dcb57b
    padding 4px 10px 3px 10px
    margin 0
    p
        float left
        font-family: 'HelveticaNeue-Bold';
        letter-spacing: 0.75px;
    form
        float right
        input
            background #fff
            border 1px solid #606060
            padding: 3px 14px 3px 8px;
            vertical-align: top;
            outline none
        button
            color: #fff;
            background: #606060;
            padding: 3px 17px 5px;
            font-size: 13px;
            vertical-align: top;
            letter-spacing: 0.75px;
            &:hover
                background darken(#606060, 10%)
.bottom-block
    padding 35px 0 10px 0
    position relative
    font-weight 400
    margin 0 0 24px 0
    ul
        float left
        li
            margin: 8px 45px 0 0;
            display: inline-block;
            a
                color: #606060;
        li
            a:hover
                color #000
                text-decoration underline
.time-order
    position absolute
    overflow hidden
    bottom -52px
    z-index 4
    right -27px
    border-radius: 33px;
    
    .text
        //display none
        display: inline-block;
        vertical-align: middle;
        position relative
        //right: -299px;
        
        color #fff
        border-radius: 33px;
        .hide-item
            background #000
            position: relative;
            top: 0;
            right: -295px;
            border-top-left-radius: 33px;
            border-bottom-left-radius: 33px;
            width: auto;
            display: inline-block;
            &:after
                content: '';
                position: absolute;
                width: 36px;
                height: 56px;
                background: #000;
                vertical-align: top;
                top: 0;
                right: -26px;
        span
            font-size: 13px;
            vertical-align: middle;
            display: inline-block;
            padding: 0 20px;
        button
            padding: 0 0 0 9px;
            position relative
            border-radius 50%
            width 56px
            height 56px
            
            &:after
                content ''
                position absolute
                border 1px solid #dcb57b
                border-radius 50%
                width: 50px;
                height: 50px;
                left: 2px;
                top: 2px;
            .icon
                width 11px
                height 20px
                padding: 0 10px;
                display inline-block
                background url(../img/icons.png) no-repeat
                background-position: -144px 0px;
    .time
        display inline-block
        vertical-align middle
        position: relative;
        z-index: 4;
        width 56px
        height 56px
        cursor pointer
        background #000
        color #fff
        border-radius 50%
        font-size 23px
        text-align: center;
        padding: 10px 0 0 0;
        line-height: 1;
        &:after
            content ''
            position absolute
            border 1px solid #dcb57b
            border-radius 50%
            width: 50px;
            height: 50px;
            left: 2px;
            top: 2px;
        strong
            font-size 9px
            display block
.slider-block.slick-slider
    width 100%
    margin 0 0 33px 0
    .slick-prev
        width 20px
        height 34px
        left 25px
        top: 184px;
        background url(../img/icons.png) no-repeat
        background-position: -94px 0px;
    .slick-next
        width 20px
        height 34px
        right 25px
        top: 184px;
        background url(../img/icons.png) no-repeat
        background-position: -117px 0px;
    .slick-dots
        bottom 20px
        li
            background #ffffff
            width 14px
            height 14px
            border-radius 50%
            vertical-align top
            margin: 0 3px;
            button:after
                display none
        .slick-active
            border 2px solid #fff
            background #dcb57b
nav
    margin 0 0 5px 0
    ul
        font-size 0
        width 100%
        &:after
            content ''
            display block
            clear both
    li
            float left
            vertical-align top
            width 20%
            font-size 0
            text-align center
           
        a
            color #fff
            padding 13px 30px
            background #606060
            font-size 18px
            display block
            &:hover
                background #dcb57b
        a.active
            background #dcb57b
.main-content
    
    .text
        float left
        width 61%
    

footer 
    padding: 21px 0px 40px 0px;
    &:after
        content ''
        display block
        clear both
    ul
        float left
        li
            width 300px
            display inline-block
            a
                color #0078ff
                text-decoration underline
        li:first-child
            margin 0 95px 0 0
    .feedback
        float right
        margin: 5px 0 0 0;
        a
            display inline-block
            width 44px
            height 44px
            background url(../img/icons.png) no-repeat
            background-position: -45px 0px;
            text-indent: 9999px;
            vertical-align: middle;
            overflow: hidden;
        .callus
            display none
            padding 16px 10px
            background #606060
            position relative
            color #fff
            margin: 0 0 0 31px;
            &:after
                content: '';
                position: absolute;
                width: 97%;
                height: 88%;
                left: 2px;
                top: 2px;
                border: 1px solid #dcb57b
        form
            display inline-block
            vertical-align middle
            margin: 0 0 0 26px;
            label
                display block
                margin-bottom: 5px;
            input
                padding 5px 10px
                width 180px
                border 1px solid #dcb57b
                outline none
            button
                background #dcb57b
                text-align center
                color #fff
                padding: 6px 5px;
                position: relative;
                left: -5px;
.overlay {
    display none
    position:fixed;
    top:0;
    left:0;
    width:100%;
    height:100%;
    z-index:2;
}                


.modal-window-calling, .modal-window-call
    position absolute
    z-index 6
    width 523px
    display none
    background #606060
    color #fff
    top 50%
    left 50%
    -moz-transform: translate(-50%, -50%) 
    -ms-transform: translate(-50%, -50%) 
    -webkit-transform: translate(-50%, -50%) 
    -o-transform: translate(-50%, -50%)
    transform: translate(-50%, -50%)  
    padding 20px 30px
    text-align center
    &:after
        content ''
        position absolute
        width 98%
        height 93%
        left 3px
        top 3px
        background none
        border 2px solid #dcb57b
    h3
        font-size 20px
        font-family: 'HelveticaNeue-Bold';
        margin 0 0 20px 0
        text-align center
        font-weight 400
        position relative
        display inline-block
        &:after
            content ''
            width 70px
            height 2px
            background #dcb57b
            position: absolute;
            bottom: -7px;
            left 15px
    .close
        position absolute
        z-index 6
        cursor pointer
        top 14px
        right 14px   
        .icon
            width: 11px;
            height: 11px;
            background-position: -1px 0px;
            display: block;
    img
        max-width 100%
.modal-window-calling
    top: 500px;
    &:after
        height 87%;
    h3
        margin: 10px 0 10px 0;
    h3:after
        display none


@import 'partials/*';
